    "type": "library",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Shepp\\LaravelTranslationReport\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Luke Sheppard",
            "email": "luke@luke-sheppard.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Shepp\\LaravelTranslationReport\\ServiceProvider"
            ]
        }
    }
}
